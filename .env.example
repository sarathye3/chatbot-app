# AI Chatbot Environment Configuration Example
# Copy this file to .env and update the values for your environment

# API Configuration - Replace with your actual API base URL
REACT_APP_API_URL=https://your-actual-api-domain.com/api

# App Configuration
REACT_APP_NAME=AI Chatbot
REACT_APP_VERSION=1.0.0

# Development Settings
REACT_APP_FORCE_MOCK_MODE=false
REACT_APP_API_TIMEOUT=10000
REACT_APP_DEBUG=false